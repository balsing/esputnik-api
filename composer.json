{
    "name": "youtool/esputnik-api-client",
    "description": "Библиотека для e-sputnik.com REST API",
    "type": "library",
    "keywords": ["esputnik", "e-sputnik.com", "api"],
    "license": "MIT",
    "config": {
        "bin-dir": "bin"
    },
    "require": {
        "php":                 ">=7.2",
        "ext-curl":            "*",
        "guzzlehttp/guzzle":   "~6.4",
        "jms/serializer":      "^2.0"
    },
    "require-dev": {
        "phpspec/phpspec": "dev-master",
        "friendsofphp/php-cs-fixer": "^2.15"
    },
    "autoload": {
        "psr-4": { "Esputnik\\": "lib/Esputnik/" }
    }
}
